extends:
    - "react-app"
    - "airbnb"
    - "prettier"
env:
    browser: true
    node: true
    jquery: true
globals:
    React: true
    TestUtils: true
    routerHelper: true
    Set: true
parser:
    "@babel/eslint-parser"
plugins:
    - "react"
settings:
    import/resolver: webpack
rules:
    "react/jsx-props-no-spreading":
        - 0
    "default-param-last":
        - 0
    "react/forbid-prop-types":
        - 1
        - forbid:
            - 'any'
            - 'array'
